            If item.ShortName = ShortName Then Return item.Specification
        Next
        Return Nothing
    End Function
    Public Function Search_Specification_ByPartData(ByRef GBData_Arr As GBDataStru(), ByRef Data_Arr As Double()) As Double()
        Dim i As Integer
        For Each item In GBData_Arr
            i = 0
            For Each d In Data_Arr
                If d <> item.Specification(i) Then Exit For
                i += 1
            Next
            If i = Data_Arr.Length Then Return item.Specification
        Next
        Return Nothing
    End Function
    Public Function Search_AorW(ByRef GBData_Arr As GBDataStru(), ByRef Data_Arr As Double(), ByRef Flag As Boolean) As Double
        'Flag = True return Weight
        'Flag = False return Area
        For Each item In GBData_Arr
            If CompareArr(item.Specification, Data_Arr) Then
                If Flag Then
                    Return item.UnitWeight
                Else
                    Return item.UnitArea
                End If
            End If
        Next
        Return Nothing
    End Function
    Private Function CompareArr(ByRef arr1 As Double(), ByRef arr2 As Double()) As Boolean
        Dim len1, len2, i As Integer
        If arr1 IsNot Nothing Then len1 = arr1.Length : If arr2 IsNot Nothing Then len2 = arr2.Length
        If len1 <> len2 Then Return False
        If len1 <> 0 Then
            For i = 0 To len1 - 1
                If arr1(i) <> arr2(i) Then Return False
                i += 1
            Next
        End If
        Return True
    End Function
End Module
